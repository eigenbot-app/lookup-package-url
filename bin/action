#!/usr/bin/env bash
# Generated by eigenbot-app/generate-go-action; DO NOT EDIT.

set -e

if ! command -v gh &>/dev/null; then
  echo "::error::Expected GitHub CLI \`gh\` in PATH."
  exit 1
fi

echo "Downloading Action \`lookup-package-url\`..."
gh release download "" \
  --repo "eigenbot-app/lookup-package-url" \
  --pattern "lookup-package-url" \
  --dir .

chmod +x "./lookup-package-url"
"./lookup-package-url" "$@"
